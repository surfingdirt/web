  "scripts": {
    "clear-babel-cache": "rm -rf ./node_modules/.cache/babel-loader",
    "build:local": "webpack --watch",
    "build": "webpack",
    "analyze": "npx webpack --profile --json > webpack-stats.json && npx webpack-bundle-analyzer webpack-stats.json dist",
    "start:local": "npm run clear-babel-cache && NODE_ENV=local npx nodemon index.js",
    "start": "node index.js",
    "storybook": "npx start-storybook --quiet --ci -p 9001",
    "build-storybook": "npx build-storybook -c .storybook",
    "test": "concurrently 'npm run storybook' 'wait-on http-get://localhost:9001 && jest' -k -s first",
    "fetch-schema": "NODE_ENV=local node server/fetch-schema.js",
    "extract-translations": "node extract.js"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "last 3 Chrome major versions",
    "last 2 Firefox major versions",
    "Safari > 10"
